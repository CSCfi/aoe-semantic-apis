{
  "name": "koodisto-service",
  "version": "1.0.0",
  "main": "src/server.ts",
  "repository": "https://kalleurhoilmari@bitbucket.org/kalleurhoilmari/koodisto-service.git",
  "author": "Kalle Lehtonen <kalle.lehtonen@digia.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "yarn run serve",
    "build": "yarn run build-ts && yarn run tslint",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn run watch-ts\" \"yarn run watch-node\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "dependencies": {
    "async-redis": "^1.1.7",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "node-cron": "^2.0.3",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "striptags": "^3.1.1",
    "xml2js": "^0.4.22"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.11",
    "@types/node": "^10.12.18",
    "@types/node-cron": "^2.0.2",
    "@types/redis": "^2.8.10",
    "@types/request": "^2.48.1",
    "@types/request-promise": "^4.1.42",
    "@types/xml2js": "^0.4.3",
    "concurrently": "^4.1.0",
    "express-swagger-generator": "^1.1.13",
    "nodemon": "^1.18.9",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  }
}
